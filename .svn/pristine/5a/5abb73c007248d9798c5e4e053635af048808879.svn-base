package com.iris.dto;

import java.io.Serializable;

import com.iris.util.Validations;

public class CustomDates implements Serializable {

	private static final long serialVersionUID = 2699469406770448908L;

	private String ytdStartDate;
	private String ytdEndDate;
	private String pqEndDate;
	private String pyEndDate;
	private String currQuStartDate;
	private String currQuEndDate;

	public String getYtdStartDate() {
		return ytdStartDate;
	}

	public void setYtdStartDate(String ytdStartDate) {
		this.ytdStartDate = Validations.trimInput(ytdStartDate);
	}

	public String getYtdEndDate() {
		return ytdEndDate;
	}

	public void setYtdEndDate(String ytdEndDate) {
		this.ytdEndDate = Validations.trimInput(ytdEndDate);
	}

	public String getPqEndDate() {
		return pqEndDate;
	}

	public void setPqEndDate(String pqEndDate) {
		this.pqEndDate = Validations.trimInput(pqEndDate);
	}

	public String getPyEndDate() {
		return pyEndDate;
	}

	public void setPyEndDate(String pyEndDate) {
		this.pyEndDate = Validations.trimInput(pyEndDate);
	}

	public String getCurrQuStartDate() {
		return currQuStartDate;
	}

	public void setCurrQuStartDate(String currQuStartDate) {
		this.currQuStartDate = currQuStartDate;
	}

	public String getCurrQuEndDate() {
		return currQuEndDate;
	}

	public void setCurrQuEndDate(String currQuEndDate) {
		this.currQuEndDate = currQuEndDate;
	}

}
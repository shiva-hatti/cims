package com.iris.dto;

import java.io.Serializable;

/**
 * @author sajadhav
 *
 */
public class DummyPanDto implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -5920561251881320038L;
	private String actualPanNumber;
	private String borrowerName;
	private String borrowerTitle;
	private String borrowerMobile;
	private String borrowerAlternateName;
	private String borrowerBob;

	public DummyPanDto() {

	}

	public DummyPanDto(String panNo, String borrowerName, String borrowerTitle, String borrowerMobile, String borrowerAlternateName) {
		this.actualPanNumber = panNo;
		this.borrowerName = borrowerName;
		this.borrowerTitle = borrowerTitle;
		this.borrowerMobile = borrowerMobile;
		this.borrowerAlternateName = borrowerAlternateName;
	}

	/**
	 * @return the borrowerName
	 */
	public String getBorrowerName() {
		return borrowerName;
	}

	/**
	 * @param borrowerName the borrowerName to set
	 */
	public void setBorrowerName(String borrowerName) {
		this.borrowerName = borrowerName;
	}

	/**
	 * @return the borrowerTitle
	 */
	public String getBorrowerTitle() {
		return borrowerTitle;
	}

	/**
	 * @param borrowerTitle the borrowerTitle to set
	 */
	public void setBorrowerTitle(String borrowerTitle) {
		this.borrowerTitle = borrowerTitle;
	}

	/**
	 * @return the borrowerMobile
	 */
	public String getBorrowerMobile() {
		return borrowerMobile;
	}

	/**
	 * @param borrowerMobile the borrowerMobile to set
	 */
	public void setBorrowerMobile(String borrowerMobile) {
		this.borrowerMobile = borrowerMobile;
	}

	/**
	 * @return the borrowerAlternateName
	 */
	public String getBorrowerAlternateName() {
		return borrowerAlternateName;
	}

	/**
	 * @param borrowerAlternateName the borrowerAlternateName to set
	 */
	public void setBorrowerAlternateName(String borrowerAlternateName) {
		this.borrowerAlternateName = borrowerAlternateName;
	}

	/**
	 * @return the borrowerBob
	 */
	public String getBorrowerBob() {
		return borrowerBob;
	}

	/**
	 * @param borrowerBob the borrowerBob to set
	 */
	public void setBorrowerBob(String borrowerBob) {
		this.borrowerBob = borrowerBob;
	}

	public String getActualPanNumber() {
		return actualPanNumber;
	}

	public void setActualPanNumber(String actualPanNumber) {
		this.actualPanNumber = actualPanNumber;
	}

}

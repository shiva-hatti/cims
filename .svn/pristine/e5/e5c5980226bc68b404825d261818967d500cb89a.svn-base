package com.iris.sdmx.dimesnsion.bean;

import java.io.Serializable;

public class DimensionBean implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private String dimConceptId;
	
	private String attachmentType;
	
	private String dimensionDesc;
	
	private String dimensionLabel;

	/**
	 * @return the dimensionLabel
	 */
	public String getDimensionLabel() {
		return dimensionLabel;
	}

	/**
	 * @param dimensionLabel the dimensionLabel to set
	 */
	public void setDimensionLabel(String dimensionLabel) {
		this.dimensionLabel = dimensionLabel;
	}

	public String getDimConceptId() {
		return dimConceptId;
	}

	public void setDimConceptId(String dimConceptId) {
		this.dimConceptId = dimConceptId;
	}

	public String getAttachmentType() {
		return attachmentType;
	}

	public void setAttachmentType(String attachmentType) {
		this.attachmentType = attachmentType;
	}

	/**
	 * @return the dimensionDesc
	 */
	public String getDimensionDesc() {
		return dimensionDesc;
	}

	/**
	 * @param dimensionDesc the dimensionDesc to set
	 */
	public void setDimensionDesc(String dimensionDesc) {
		this.dimensionDesc = dimensionDesc;
	}
	
	

}

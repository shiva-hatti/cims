package com.iris.sdmx.dataCollection.bean;

/**
 * @author apagaria
 *
 */
public class SDMXDataCollectionLimitedFieldBean {

	/**
	 * 
	 */
	private String dataCode;

	/**
	 * 
	 */
	private String dataLable;

	/**
	 * 
	 */
	private String categoryLable;

	/**
	 * 
	 */
	public SDMXDataCollectionLimitedFieldBean() {
	}

	/**
	 * @param dataCode
	 * @param dataLable
	 * @param categoryLable
	 */
	public SDMXDataCollectionLimitedFieldBean(String dataCode, String dataLable, String categoryLable) {
		this.dataCode = dataCode;
		this.dataLable = dataLable;
		this.categoryLable = categoryLable;
	}

	/**
	 * @return the dataCode
	 */
	public String getDataCode() {
		return dataCode;
	}

	/**
	 * @param dataCode the dataCode to set
	 */
	public void setDataCode(String dataCode) {
		this.dataCode = dataCode;
	}

	/**
	 * @return the dataLable
	 */
	public String getDataLable() {
		return dataLable;
	}

	/**
	 * @param dataLable the dataLable to set
	 */
	public void setDataLable(String dataLable) {
		this.dataLable = dataLable;
	}

	/**
	 * @return the categoryLable
	 */
	public String getCategoryLable() {
		return categoryLable;
	}

	/**
	 * @param categoryLable the categoryLable to set
	 */
	public void setCategoryLable(String categoryLable) {
		this.categoryLable = categoryLable;
	}

	/**
	 *
	 */
	@Override
	public String toString() {
		return "SDMXDataCollectionLimitedFieldBean [dataCode=" + dataCode + ", dataLable=" + dataLable + ", categoryLable=" + categoryLable + "]";
	}
}

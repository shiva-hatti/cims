package com.iris.dto;

import java.io.Serializable;
import java.util.Date;

/**
 * this class is bean class for MenuRoleMap Management module
 * @author Sanjayv
 * version 1.0
 */

public class MenuRoleMapDto implements Serializable {

	private static final long serialVersionUID = -4167862181529537516L;

	private Long menuRoleId;
	
	private Long userRoleIdFk;
	
	private Long menuIDFk;
	
	private Boolean isActive;
	
	private Long modifiedBy;
	
	private Date modifiedOn;

	public Long getMenuRoleId() {
		return menuRoleId;
	}

	public void setMenuRoleId(Long menuRoleId) {
		this.menuRoleId = menuRoleId;
	}

	public Long getUserRoleIdFk() {
		return userRoleIdFk;
	}

	public void setUserRoleIdFk(Long userRoleIdFk) {
		this.userRoleIdFk = userRoleIdFk;
	}

	public Long getMenuIDFk() {
		return menuIDFk;
	}

	public void setMenuIDFk(Long menuIDFk) {
		this.menuIDFk = menuIDFk;
	}

	public Boolean getIsActive() {
		return isActive;
	}

	public void setIsActive(Boolean isActive) {
		this.isActive = isActive;
	}

	public Long getModifiedBy() {
		return modifiedBy;
	}

	public void setModifiedBy(Long modifiedBy) {
		this.modifiedBy = modifiedBy;
	}

	public Date getModifiedOn() {
		return modifiedOn;
	}

	public void setModifiedOn(Date modifiedOn) {
		this.modifiedOn = modifiedOn;
	}
	
	
}

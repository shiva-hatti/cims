package com.iris.model;

import java.util.List;

/**
 * @author apagaria
 *
 */
public class ErrorCodeLabelRequest {

	List<Long> errorCodeIdList;

	Boolean isActive;

	Boolean isWebBasedLabelOnly;

	Boolean isFileBasedLabelOnly;

	/**
	 * @return the errorCodeIdList
	 */
	public List<Long> getErrorCodeIdList() {
		return errorCodeIdList;
	}

	/**
	 * @param errorCodeIdList the errorCodeIdList to set
	 */
	public void setErrorCodeIdList(List<Long> errorCodeIdList) {
		this.errorCodeIdList = errorCodeIdList;
	}

	/**
	 * @return the isActive
	 */
	public Boolean getIsActive() {
		return isActive;
	}

	/**
	 * @param isActive the isActive to set
	 */
	public void setIsActive(Boolean isActive) {
		this.isActive = isActive;
	}

	/**
	 * @return the isWebBasedLabelOnly
	 */
	public Boolean getIsWebBasedLabelOnly() {
		return isWebBasedLabelOnly;
	}

	/**
	 * @param isWebBasedLabelOnly the isWebBasedLabelOnly to set
	 */
	public void setIsWebBasedLabelOnly(Boolean isWebBasedLabelOnly) {
		this.isWebBasedLabelOnly = isWebBasedLabelOnly;
	}

	/**
	 * @return the isFileBasedLabelOnly
	 */
	public Boolean getIsFileBasedLabelOnly() {
		return isFileBasedLabelOnly;
	}

	/**
	 * @param isFileBasedLabelOnly the isFileBasedLabelOnly to set
	 */
	public void setIsFileBasedLabelOnly(Boolean isFileBasedLabelOnly) {
		this.isFileBasedLabelOnly = isFileBasedLabelOnly;
	}

	@Override
	public String toString() {
		return "ErrorCodeLabelRequest [errorCodeIdList=" + errorCodeIdList + ", isActive=" + isActive + ", isWebBasedLabelOnly=" + isWebBasedLabelOnly + ", isFileBasedLabelOnly=" + isFileBasedLabelOnly + "]";
	}
}

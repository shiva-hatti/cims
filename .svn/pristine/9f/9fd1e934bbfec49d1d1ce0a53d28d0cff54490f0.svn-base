package com.iris.sdmx.model.code.data;

import java.io.Serializable;

public class SdmxDataModelCodeRequestBean implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -3581894010879225222L;
	
	private Long userId;
	
	private Long roleId;
	
	private Boolean isApprovedRecord;

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public Long getRoleId() {
		return roleId;
	}

	public void setRoleId(Long roleId) {
		this.roleId = roleId;
	}

	public Boolean getIsApprovedRecord() {
		return isApprovedRecord;
	}

	public void setIsApprovedRecord(Boolean isApprovedRecord) {
		this.isApprovedRecord = isApprovedRecord;
	}
	
	

}

package com.iris.model;

/**
 * @author apagaria
 *
 */
public class DateAndTimeFormatResponse {

	private String activeDateFormat;

	private String activeTimeFormat;

	/**
	 * @return the activeDateFormat
	 */
	public String getActiveDateFormat() {
		return activeDateFormat;
	}

	/**
	 * @param activeDateFormat the activeDateFormat to set
	 */
	public void setActiveDateFormat(String activeDateFormat) {
		this.activeDateFormat = activeDateFormat;
	}

	/**
	 * @return the activeTimeFormat
	 */
	public String getActiveTimeFormat() {
		return activeTimeFormat;
	}

	/**
	 * @param activeTimeFormat the activeTimeFormat to set
	 */
	public void setActiveTimeFormat(String activeTimeFormat) {
		this.activeTimeFormat = activeTimeFormat;
	}
}

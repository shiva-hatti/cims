package com.iris.util.constant;

public enum MailConstant {

	REGULATOR(1L), COMPANY(2L), AUDITOR(3L), EMAIL_TO(1), EMAIL_CC(2), EMAIL_BCC(3), NBFC_VERIFICATION_EMAIL_ALERT_ID(96L), NBFC_USER_CREATED_EMAIL_ALERT_ID(98L);

	private Long number;
	private Integer no;

	private MailConstant(Long number) {
		this.number = number;
	}

	private MailConstant(Integer no) {
		this.no = no;
	}

	public Long getConstantLongVal() {
		return number;
	}

	public Integer getConstantIntVal() {
		return no;
	}
}
